<launch>

  <node pkg="v4l2_camera" exec="v4l2_camera_node" output="screen" respawn="false">
  </node>

  <!--name argument is not required, because it will overwrite all node name in the exec-->
  <node pkg="lego_loam_bor" exec="yolo_ros2_image_sub" output="screen" respawn="false">
    <param name="trt_model_path" value="$(find-pkg-share lego_loam_bor)/engine/yolo11n-seg.trt" />
    <remap from="/image" to="/image_raw"/>
  </node>

  <node pkg="rviz2" exec="rviz2" output="screen" respawn="false" args="-d $(find-pkg-share lego_loam_bor)/rviz/yolo_image_sub_test.rviz">
  </node>

</launch>
